{
  "name": "zorroa-js-curator",
  "version": "0.3.0",
  "description": "this is a front end template using react, redux, and webpack built by jordan with love",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zorroa/zorroa-js-curator.git"
  },
  "scripts": {
    "post-install": "rm -rf bin; npm run build",
    "start": "./node_modules/.bin/serve ./bin -p 8080",
    "dev": "./node_modules/.bin/webpack-dev-server --config ./build/webpack.dev.js --progress --inline --hot --colors --no-info --history-api-fallback",
    "build-dev": "./node_modules/.bin/webpack --config ./build/webpack.dev.js --progress --colors",
    "build-only": "./node_modules/.bin/webpack --config ./build/webpack.prod.js --progress --colors",
    "build-prodlocal": "./node_modules/.bin/webpack --config ./build/webpack.prodlocal.js --progress --colors",
    "build": "npm test && npm run build-only",
    "deploy": "sh ./build/build-prod.sh",
    "pretest": "npm run test:lint",
    "test": "./node_modules/.bin/jest -i",
    "test:watch": "./node_modules/.bin/jest --verbose --watch",
    "test:e2e": "./node_modules/.bin/jest e2e",
    "test:lint": "./node_modules/.bin/eslint src --color; exit 0",
    "test:coverage": "npm test; ./node_modules/.bin/serve ./coverage/lcov-report & open http://localhost:8080",
    "purge": "rm -rf node_modules; npm cache clean; npm install",
    "update-fonts": "src/assets/fonts/zorroa-icons/update-fonts.bash",
    "preview-fonts": "src/assets/fonts/zorroa-icons/preview-fonts.bash"
  },
  "author": "Jordan Papaleo",
  "license": "",
  "engines": {
    "node": "6.3.1",
    "npm": "3.10.7"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "<rootDir>/tests/StyleStub.js",
      "^.+\\.(jpg|png|jpeg|svg)$": "<rootDir>/tests/MediaStub.js"
    },
    "collectCoverage": true
  },
  "devDependencies": {
    "axios-mock-adapter": "1.6.1",
    "babel-cli": "6.24.0",
    "babel-core": "6.14.0",
    "babel-eslint": "7.2.1",
    "babel-jest": "16.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-node6": "11.0.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-1": "6.1.18",
    "babel-register": "6.14.0",
    "chromedriver": "2.25.1",
    "css-loader": "0.25.0",
    "enzyme": "2.4.1",
    "eslint": "3.18.0",
    "eslint-config-standard": "6.2.1",
    "eslint-loader": "1.7.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "6.10.3",
    "file-loader": "0.9.0",
    "geckodriver": "1.2.0",
    "handlebars-webpack-plugin": "0.1.1",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "jest-cli": "18.1.0",
    "json": "9.0.4",
    "json-loader": "0.5.4",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "node-sass": "3.9.3",
    "raw-loader": "0.5.1",
    "react-addons-test-utils": "15.4.2",
    "react-mapbox-gl": "0.17.0",
    "react-test-renderer": "15.3.2",
    "redux-devtools": "3.3.1",
    "sass-loader": "4.0.2",
    "saucelabs": "1.3.0",
    "selenium-webdriver": "3.0.1",
    "serve": "1.4.0",
    "standard": "8.0.0",
    "style-loader": "0.13.1",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2"
  },
  "dependencies": {
    "axios": "0.14.0",
    "classnames": "2.2.5",
    "copy-to-clipboard": "3.0.5",
    "debug": "2.2.0",
    "latlon-geohash": "1.1.0",
    "moment": "2.17.1",
    "nightwatch": "0.9.14",
    "react": "15.4.2",
    "react-addons-css-transition-group": "15.3.2",
    "react-dom": "15.4.2",
    "react-json-tree": "0.10.7",
    "react-keydown": "1.6.3",
    "react-measure": "1.2.0",
    "react-pdf-js": "1.0.33",
    "react-player": "0.12.0",
    "react-redux": "4.4.5",
    "react-router": "2.8.0",
    "react-widgets": "3.4.5",
    "recharts": "0.18.0",
    "redux": "3.6.0",
    "redux-mock-store": "1.2.1",
    "redux-multi": "0.1.12",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.1.0"
  }
}
