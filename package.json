{
  "name": "zorroa-js-curator",
  "version": "0.40.0",
  "description": "this is a front end template using react, redux, and webpack built by jordan with love",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zorroa/zorroa-js-curator.git"
  },
  "scripts": {
    "prepush": "npm test",
    "post-install": "rm -rf bin; npm run build",
    "start": "./node_modules/.bin/serve ./bin -p 8081",
    "dev": "./node_modules/.bin/webpack-dev-server --config ./build/webpack.dev.js --progress --inline --hot --colors --no-info --history-api-fallback",
    "dev-https": "./node_modules/.bin/webpack-dev-server --config ./build/webpack.dev.js --progress --inline --hot --colors --no-info --history-api-fallback --https --cert ./cert.pem --key ./key.pem # this is for safari testing",
    "dev-https-cert": "openssl req -x509 -newkey rsa:4096 -keyout key.pem -out cert.pem -days 365 -nodes -subj '/CN=localhost'",
    "build-dev": "./node_modules/.bin/webpack --config ./build/webpack.dev.js --progress --colors",
    "build-only": "./node_modules/.bin/webpack --config ./build/webpack.prod.js --progress --colors",
    "build-prodlocal": "./node_modules/.bin/webpack --config ./build/webpack.prodlocal.js --progress --colors",
    "build": "npm test && npm run build-only",
    "deploy": "sh ./build/build-prod.sh",
    "test": "npm run test:lint && npm run test:unit",
    "test:watch": "$(npm bin)/jest src/**/*.test.js --verbose --watch",
    "test:lint": "$(npm bin)/eslint src --color",
    "test:unit": "$(npm bin)/jest src/**/*.test.js",
    "purge": "rm -rf node_modules; npm cache clean; npm install",
    "update-fonts": "src/assets/fonts/zorroa-icons/update-fonts.bash",
    "preview-fonts": "src/assets/fonts/zorroa-icons/preview-fonts.bash"
  },
  "author": "zorroa.com",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "7.10.1",
    "npm": "^3.10.7"
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "<rootDir>/tests/StyleStub.js",
      "^.+\\.(jpg|png|jpeg|svg)$": "<rootDir>/tests/MediaStub.js"
    },
    "collectCoverage": true
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid"
  },
  "devDependencies": {
    "axios-mock-adapter": "1.9.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "16.0.0",
    "babel-loader": "6.4.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "babel-register": "6.26.0",
    "chromedriver": "2.36.0",
    "css-loader": "0.25.0",
    "enzyme": "2.9.1",
    "eslint": "3.19.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-standard": "6.2.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-prettier": "2.5.0",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-react": "6.10.3",
    "eslint-plugin-standard": "2.3.1",
    "file-loader": "0.9.0",
    "handlebars-webpack-plugin": "0.1.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.14.3",
    "install": "0.11.0",
    "jest-cli": "18.1.0",
    "json": "9.0.6",
    "json-loader": "0.5.7",
    "less": "2.7.2",
    "less-loader": "2.2.3",
    "node-sass": "3.13.1",
    "npm": "5.8.0",
    "pdfjs-dist": "1.10.97",
    "prettier": "1.10.2",
    "raw-loader": "0.5.1",
    "react-mapbox-gl": "0.17.0",
    "react-test-renderer": "15.6.2",
    "sass-loader": "4.1.1",
    "selenium-webdriver": "3.6.0",
    "serve": "1.4.0",
    "style-loader": "0.13.2",
    "webpack": "1.15.0",
    "webpack-dev-server": "1.16.5"
  },
  "dependencies": {
    "axios": "0.14.0",
    "camel-case": "3.0.0",
    "classnames": "2.2.5",
    "copy-to-clipboard": "3.0.8",
    "debug": "2.6.9",
    "decamelize": "https://registry.npmjs.org/decamelize/-/decamelize-1.2.0.tgz",
    "downloadjs": "1.4.7",
    "dropbox": "2.5.12",
    "ease-component": "1.0.0",
    "latlon-geohash": "1.1.0",
    "lru-cache": "4.1.1",
    "moment": "2.21.0",
    "react": "15.6.2",
    "react-addons-css-transition-group": "15.6.2",
    "react-addons-transition-group": "15.6.2",
    "react-axis": "0.2.5",
    "react-dom": "15.6.2",
    "react-json-tree": "0.10.9",
    "react-keydown": "1.9.4",
    "react-masonry-component": "6.0.2",
    "react-measure": "1.4.7",
    "react-redux": "4.4.8",
    "react-router-dom": "4.2.2",
    "react-widgets": "3.5.0",
    "recharts": "0.18.0",
    "redux": "3.7.2",
    "redux-mock-store": "1.3.0",
    "redux-multi": "0.1.12",
    "redux-promise": "0.5.3",
    "redux-thunk": "2.2.0"
  }
}
